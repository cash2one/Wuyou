<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.istudy.server.cms.service.CityService">
	 <select id="getAllCitys" resultType="com.istudy.server.cms.vo.CityVO">
	 	<![CDATA[
	 	 SELECT 
	 	 id,
	 	 city_id as cityId,
	 	 city_name as cityName,
	 	 province_id as provinceId
	 	 FROM cms_city_info
	 	]]>
	 </select>
</mapper>